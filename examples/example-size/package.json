{
  "name": "react-webpack-start",
  "version": "1.0.0",
  "description": "Wepack React with Babel, CSS Modules, ESLint, Jest, Enzyme",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "yarn clean && webpack-dev-server --inline --no-info",
    "build": "yarn clean && NODE_ENV=production webpack-cli",
    "explore": "yarn source-map-explorer dist/main.*.js dist/main.*.js.map"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-webpack-starter.git"
  },
  "author": "Samuel Vaillant <samuel.vllnt@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "core-js": "2.5.4",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.1.0",
    "source-map-explorer": "1.5.0",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "3.1.1"
  },
  "dependencies": {
    "package-dom": "1.0.0",
    "package-native": "1.0.0",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  }
}
