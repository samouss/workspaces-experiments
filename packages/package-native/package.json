{
  "name": "package-native",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "watch": "yarn build:cjs --watch",
    "prepare": "yarn build:cjs",
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "babel src --out-dir dist --quiet",
    "build:es": "BABEL_ENV=es babel src --out-dir dist/es --quiet"
  }
}
